{
  "name": "simple-ioc-container",
  "version": "1.0.0",
  "description": "simple ioc container using typescript",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register src/index.ts",
    "test:repository": "jest ./user/user.repository.spec.ts --watch",
    "test:service": "jest ./user/user.service.spec.ts --watch",
    "test:controller": "jest ./user/user.controller.spec.ts --watch",
    "test:watch": "jest --watchAll",
    "test": "jest"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "28.1.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.0",
    "jest": "28.1.1",
    "mocha": "10.0.0",
    "ts-jest": "28.0.5",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13"
  }
}
